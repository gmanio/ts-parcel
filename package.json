{
  "name": "ts-parcel",
  "version": "1.0.0",
  "description": "I want to know How faster parcel than webpack,",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "parcel ./src/index.html --no-cache",
    "build": "parcel build src/index.html --public-url . --no-cache",
    "build:board": "parcel build src/containers/board/board.html --public-url . --no-cache",
    "deploy": "firebase deploy"
  },
  "author": "GmanPark",
  "license": "MIT",
  "devDependencies": {
    "@types/fabric": "^1.5.27",
    "autoprefixer": "^7.2.3",
    "fabric": "^2.0.0-rc.4",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.4.1",
    "postcss-modules": "^1.1.0",
    "postcss-sprites": "^4.2.1",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "firebase": "^4.8.1"
  }
}
